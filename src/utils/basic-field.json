[
  {
    "label": "单行文本",
    "type": "input",
    "wrapper": "basic",
    "acctionAttributeControl": ["hidden", "disabled", "password"],
    "formRulesControl": ["required", "dataType", "pattern"]
  },
  {
    "label": "多行文本",
    "type": "textarea",
    "wrapper": "basic",
    "acctionAttributeControl": ["hidden", "disabled"],
    "formRulesControl": ["required", "pattern"]
  },
  {
    "label": "计数器",
    "type": "input-number",
    "wrapper": "basic",
    "acctionAttributeControl": ["hidden", "disabled"],
    "formRulesControl": ["required"]
  },
  {
    "label": "单选框组",
    "type": "radio-group",
    "wrapper": "basic",
    "options": [
      { "label": "Option1", "value": "option1" },
      { "label": "Option2", "value": "option2" },
      { "label": "Option3", "value": "option3" }
    ],
    "prop": {
      "label": "label",
      "value": "value"
    },
    "mode": "default"
  },
  {
    "label": "多选框组",
    "type": "checkbox-group",
    "wrapper": "basic",
    "options": [
      { "label": "Option1", "value": "option1" },
      { "label": "Option2", "value": "option2" },
      { "label": "Option3", "value": "option3" }
    ],
    "prop": {
      "label": "label",
      "value": "value"
    },
    "mode": "multiple",
    "defaultValue": []
  },
  {
    "label": "下拉选择框",
    "type": "select",
    "wrapper": "basic",
    "options": [
      { "label": "Option1", "value": "option1" },
      { "label": "Option2", "value": "option2" },
      { "label": "Option3", "value": "option3" }
    ],
    "prop": {
      "label": "label",
      "value": "value"
    },
    "mode": "default",
    "defaultValue": []
  },
  {
    "label": "时间选择器",
    "type": "time-picker",
    "wrapper": "basic",
    "format": "HH:mm:ss"
  },
  {
    "label": "日期选择器",
    "type": "date-picker",
    "wrapper": "basic",
    "format": "YYYY-MM-DD"
  },
  {
    "label": "评分",
    "type": "rate",
    "wrapper": "basic",
    "count": 5
  },
  {
    "label": "开关",
    "type": "switch",
    "wrapper": "basic"
  },
  {
    "label": "滑块",
    "type": "slider",
    "wrapper": "basic",
    "min": 0,
    "max": 100,
    "step": 1,
    "defaultValue": 0
  },
  {
    "label": "文字",
    "type": "text",
    "wrapper": "basic",
    "defaultValue": "This is a text"
  },
  {
    "label": "HTML",
    "type": "html",
    "wrapper": "basic",
    "defaultValue": "<strong>Test HTML</strong>",
    "acctionAttributeControl": ["hidden"]
  }
]
